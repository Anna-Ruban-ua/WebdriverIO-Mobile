  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@types/node": "^22.15.2",
    "@wdio/allure-reporter": "^9.12.6",
    "@wdio/appium-service": "^9.12.7",
    "@wdio/browserstack-service": "^9.12.7",
    "@wdio/cli": "^9.12.7",
    "@wdio/globals": "^9.12.7",
    "@wdio/local-runner": "^9.12.7",
    "@wdio/mocha-framework": "^9.12.6",
    "@wdio/spec-reporter": "^9.12.6",
    "appium": "^2.18.0",
    "appium-uiautomator2-driver": "^4.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "test:bs:pixel3": "wdio run wdio.pixel3.conf.ts",
